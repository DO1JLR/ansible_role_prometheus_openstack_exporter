{{ ansible_managed | comment }}

[Unit]
Description=golang openstack exporter service
After=network-online.target

[Service]
Type=simple
User={{ openstack_exporter__user }}
Group={{ openstack_exporter__group }}
ExecStart=/usr/local/bin/openstack-exporter --os-client-config="/etc/openstack/clouds.yaml" --web.listen-address=":9180" wingcon
Restart=always
RestartSec=1


[Install]
WantedBy=multi-user.target
